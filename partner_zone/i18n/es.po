"        <field name=\"name\">message_index_list_view_delegate_open</field>\n"
"        <field name=\"model_id\" ref=\"model_message_index\"/>\n"
"        <field name=\"code\">action = object.button_popup_delegate()</"
"field>\n"
"    </record>\n"
"\n"
"    <!-- tree_but_open key -- delegates to a server action to execute on row "
